<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FSTTests" thread-count="4" parallel="classes">
    <!-- Comment out groups to run everything -->
	<groups>
        <run>
            <include name="HeaderTests" />
        </run>
    </groups>
    <parameter name="username" value="admin" />
    <parameter name="password" value="password" />
    <test name="FSTActivities">
        <classes>
            <class name="TestNG_Activity.Activity1_TestNG" />
            <class name="TestNG_Activity.Activity2_TestNG" />
            <class name="TestNG_Activity.Activity3_TestNG" />
            <class name="TestNG_Activity.Activity4_TestNG_Class1" />
            <class name="TestNG_Activity.Activity4_TestNG_Class2" />
            <class name="TestNG_Activity.Activity5_TestNG" />
            <class name="TestNG_Activity.Activity6_TestNG" />
            <class name="TestNG_Activity.Activity7_TestNG" />
        </classes>
    </test>
</suite>